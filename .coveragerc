# Configuração do Coverage.py

[run]
source = .
omit = 
    */tests/*
    */test_*.py
    */migrations/*
    */venv/*
    */env/*
    manage.py
    */asgi.py
    */wsgi.py
    */settings.py
    */setup.py

[report]
precision = 2
show_missing = True
skip_covered = False

# Ignorar linhas específicas
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

[html]
directory = htmlcov

